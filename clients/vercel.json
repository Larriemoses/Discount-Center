{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://discount-center.onrender.com/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://discount-center.onrender.com/uploads/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
